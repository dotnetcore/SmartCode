@using SmartCode
@using SmartCode.Generator.Entity
@model BuildContext
@{
    var project = Model.Project;
    var buildTask = Model.Build;
    var table = Model.GetCurrentTable();
}
using System.Threading.Tasks;
using @(project.Author).@(project.Module).Models;
using @(project.Author).@(project.Module).Services;
using NUnit.Framework;

namespace @(project.Author).@(project.Module).@(buildTask.Module).Services {

    [TestFixture]
    public class @(table.ConvertedName)ServiceTest : BaseTest<I@(table.ConvertedName)Service> {

        [Test]
        public void _01_CanResolveTarget() {
            Assert.IsNotNull(Target);
        }

        [Test]
        public async Task _02_CanSearch() {
            var model = new @(table.ConvertedName)SearchModel {
                Skip = 0,
                Take = 10
            };
            var result = await Target.SearchAsync(model);
            Assert.IsNotNull(result);
            Assert.AreEqual(model.Skip, result.Skip);
            Assert.AreEqual(model.Take, result.Take);
            Assert.IsNotNull(result.Data);
            Assert.GreaterOrEqual(model.Take, result.Data.Count);
        }

    }

}
